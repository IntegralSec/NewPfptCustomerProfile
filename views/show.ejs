    <!-- POC UPDATE -->
    <div class="modal fade" id="editPOC" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title" id="lineModalLabel">POC/UAT information</h3>
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span
                                class="sr-only">Close</span></button>
                </div>
                <div class="modal-body">

                    <!-- content goes here -->
                    <form class=""
                          action="/index/<%= customer.name %>?_method=PUT"
                          method="POST">
                        <div class="form-group">
                            <label for="customerName">Customer Name</label>
                            <input type="text" readonly class="form-control" placeholder="No response set"
                                   value="<%= customer.name %>"
                                   name="poc_questions[name]">
                        </div>
                        <h5 style="color: mediumvioletred">POC</h5>
                        <div class="form-group">
                            <label for="sandbox_poc">Sandbox POC</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= poc_questions.POC.is_sandbox_poc %>"
                                   name="poc_questions[POC][is_sandbox_poc]">
                        </div>

                        <div class="form-group">
                            <label>Prod Based POC</label>
                            <input type="text" class="form-control" placeholder="No response set" id="prod_based_poc"
                                   value="<%= poc_questions.POC.is_prod_poc %>"
                                   name="poc_questions[POC][is_prod_poc]">
                        </div>
                        <div class="form-group">
                            <label>What will be tested in the POC?</label>
                            <input type="text" class="form-control" placeholder="No response set" id="poc_tests"
                                   value="<%= poc_questions.POC.poc_tests %>"
                                   name="poc_questions[POC][poc_tests]">
                        </div>
                        <div class="form-group">
                            <label>Has the customer agreed to Success Criteria?</label>
                            <input type="text" class="form-control" placeholder="No response set" id="agreed_to_success_criteria"
                                   value="<%= poc_questions.POC.agreed_to_success_criteria %>"
                                   name="poc_questions[POC][agreed_to_success_criteria]">
                        </div>
                        <div class="form-group">
                            <label>Environments needed? (POC, UAT)</label>
                            <input type="text" class="form-control" placeholder="No response set" id="envs_needed"
                                   value="<%= poc_questions.POC.envs_needed %>"
                                   name="poc_questions[POC][envs_needed]">
                        </div>
                        <div class="form-group">
                            <label>How long do you need them for?</label>
                            <input type="text" class="form-control" placeholder="No response set" id="how_long_needed"
                                   value="<%= poc_questions.POC.how_long_needed %>"
                                   name="poc_questions[POC][how_long_needed]">
                        </div>
                        <div class="form-group">
                            <label>How to convert the environment and/or data to PROD?</label>
                            <input type="text" class="form-control" placeholder="No response set" id="how_to_convert_env"
                                   value="<%= poc_questions.POC.how_to_convert_env %>"
                                   name="poc_questions[POC][how_to_convert_env]">
                        </div>
                        <div class="form-group">
                            <label>Will you be installing appliances in production or in a
                                lab?</label>
                            <input type="text" class="form-control" placeholder="No response set" id="installing_in_production_or_lab"
                                   value="<%= poc_questions.POC.installing_in_production_or_lab %>"
                                   name="poc_questions[POC][installing_in_production_or_lab]">
                        </div>
                        <div class="form-group">
                            <label>For PROD, how many environments are needed and
                                locations?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= poc_questions.POC.how_many_envs %>"
                                   name="poc_questions[POC][how_many_envs]">
                        </div>
                        <div class="form-group">
                            <label>How many mailboxes will you journal into the POC?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= poc_questions.POC.how_many_mailboxes %>"
                                   name="poc_questions[POC][how_many_mailboxes]">
                        </div>
                        <h5 style="color: mediumvioletred">UAT</h5>
                        <div class="form-group">
                            <label>Does the customer need/require a UAT or Testing Archive
                                account?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= poc_questions.UAT.need_UAT %>"
                                   name="poc_questions[UAT][need_UAT]">
                        </div>
                        <div class="form-group">
                            <label>Will the UAT be forever?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= poc_questions.UAT.UAT_forever %>"
                                   name="poc_questions[UAT][UAT_forever]">
                        </div>
                        <div class="form-group">
                            <label>Will the UAT be production data, or lab data?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= poc_questions.UAT.is_UAT_production_or_lab %>"
                                   name="poc_questions[UAT][is_UAT_production_or_lab]">
                        </div>
                        <div class="form-group">
                            <label>What is the primary purpose of the UAT?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= poc_questions.UAT.primary_purpose %>"
                                   name="poc_questions[UAT][primary_purpose]">
                        </div>
                        <div class="col-4">
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <div class="btn-group btn-group-justified" role="group" aria-label="group button">
                        <div class="btn-group" role="group">
                            <button type="button" class="btn btn-danger" data-dismiss="modal" role="button">Close
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

