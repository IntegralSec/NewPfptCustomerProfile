<!-- Displaying Email Systems SE questions -->
<div id="emailSystemSe" class="display-header">
    <h5><a href="#emailContainer" data-toggle="collapse">Email Systems SE
            <i class="fa fa-arrows-v" aria-hidden="true"></i></a></h5>
    <br>

    <div id="emailContainer" class="collapse">
        <div class="edit pull-right">
            <a href="#/" id="editEmailSystemSELink" aria-hidden="true" data-toggle="modal"><i
                        class="fa fa-pencil-square-o"> Edit</i></a>
        </div>
        <br>

        <form autocomplete="off" id="emailSystemSEForm" action="/index/<%= encodeURIComponent(customer.name) %>?_method=PUT" method="POST">
            <fieldset id="emailSystemSEFormFieldset" disabled>
                <div class="form-group col-4 response-cell" hidden>
                    <label>Customer Name</label>
                    <input type="text" readonly class="form-control" placeholder="No response set"
                           value="<%= customer.name %>"
                           name="email_se_questions[name]">
                </div>

                <h5><a href="#editEmailSystemsSE-1" style="color: mediumvioletred" data-toggle="collapse">On-prem Exchange w/ On-prem Appliances
                        <i class="fa fa-arrows-v" aria-hidden="true"></i></a></h5>

                <div id="editEmailSystemsSE-1" class="collapse">
                    <h5 style="color: lightseagreen">Exchange Details</h5>
                    <div class="row">
                        <div class="form-group col-4 response-cell"><label>What versions of Exchange are running?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= email_se_questions.prem_exchange.exchange_version %>"
                                   name="email_se_questions[prem_exchange][exchange_version]">
                        </div>

                        <div class="form-group col-4 response-cell"><label>How many locations are mailbox servers deployed?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= email_se_questions.prem_exchange.number_of_mail_servers %>"
                                   name="email_se_questions[prem_exchange][number_of_mail_servers]">
                        </div>

                        <div class="form-group col-4 response-cell"><label>Do you have an Enterprise CAL?</label>
                            <%- make_custom_dropdown("email_se_questions[prem_exchange][has_enterprise_CAL]",
                                    email_se_questions.prem_exchange.has_enterprise_CAL, ["Yes", "No"]) %>
                        </div>

                        <div class="form-group col-4 response-cell"><label style="color: red;">Are you replicating Exchange and AD to a secondary data
                                center?</label>
                            <%- make_custom_dropdown("email_se_questions[prem_exchange][is_replicating_exchange_and_AD]",
                                    email_se_questions.prem_exchange.is_replicating_exchange_and_AD, ["Yes", "No"]) %>
                        </div>

                        <div class="form-group col-4 response-cell"><label>Please provide a diagram of your exchange infrastructure and your AD
                                Details.</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= email_se_questions.prem_exchange.diagram %>"
                                   name="email_se_questions[prem_exchange][diagram]">
                        </div>
                    </div>
                    <br>

                    <h5 style="color: lightseagreen">Active Directory Details</h5>
                    <div class="row">
                        <div class="form-group col-4 response-cell"><label>Is your AD multi-forest?</label>
                            <%- make_custom_dropdown("email_se_questions[prem_exchange][is_multi_forest]",
                                    email_se_questions.prem_exchange.is_multi_forest, ["Yes", "No"]) %>
                        </div>
                        <div class="form-group col-4 response-cell"><label>Are there trusts between the forests?</label>
                            <%- make_custom_dropdown("email_se_questions[prem_exchange][exists_trusts]",
                                    email_se_questions.prem_exchange.exists_trusts, ["Yes", "No"]) %>
                        </div>
                        <div class="form-group col-4 response-cell"><label>Is this a Resource Forest arrangement?</label>
                            <%- make_custom_dropdown("email_se_questions[prem_exchange][is_resource_forest_arrangement]",
                                    email_se_questions.prem_exchange.is_resource_forest_arrangement, ["Yes", "No"]) %>
                        </div>
                        <div class="form-group col-4 response-cell"><label>Are you using multi-domains?</label>
                            <%- make_custom_dropdown("email_se_questions[prem_exchange][is_using_multi_domains]",
                                    email_se_questions.prem_exchange.is_using_multi_domains, ["Yes", "No"]) %>
                        </div>
                        <div class="form-group col-4 response-cell"><label>If yes, what is the structure of multi-domain architecture / their
                                relationships?</label>
                            <input type="text" class="form-control" placeholder="No response set"
                                   value="<%= email_se_questions.prem_exchange.structure %>"
                                   name="email_se_questions[prem_exchange][structure]">
                        </div>
                    </div>
                    <br>
                </div>

                <h5><a href="#editEmailSystemsSE-2" style="color: mediumvioletred" data-toggle="collapse">O365 (Hosted Appliances)
                        <i class="fa fa-arrows-v" aria-hidden="true"></i></a></h5>

