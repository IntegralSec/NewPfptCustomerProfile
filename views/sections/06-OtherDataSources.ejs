                                <div class="form-group, col-4, response-checkbox"><p><b>Google </b>
                                        <input type="checkbox"
                                               name="other_data_source_questions[public_social_archiving][need_social_media_content][google]"
                                                <%= other_data_source_questions.public_social_archiving.need_social_media_content.google ?
                                                        "checked" : "" %> />
                                    </p></div>
                                <div class="form-group, col-4, response-checkbox"><p><b>YouTube </b>
                                        <input type="checkbox"
                                               name="other_data_source_questions[public_social_archiving][need_social_media_content][youtube]"
                                                <%= other_data_source_questions.public_social_archiving.need_social_media_content.youtube ?
                                                        "checked" : "" %> />
